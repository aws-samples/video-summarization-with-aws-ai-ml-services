{
  "name": "video-summarization-hub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ignore-path .gitignore --ext ts,tsx,js",
    "config": "node cli.js generate-config",
    "frontend:deploy": "aws s3 cp --recursive dist/ s3://$(npm run-script echo-ui-bucket --silent)",
    "echo-video-bucket": "node cli.js echo-video-bucket",
    "echo-ui-bucket": "node cli.js echo-bucket",
    "echo-ui-url": "node cli.js echo-url",
    "deploy": "npm-run-all -s sam-build sam-deploy config build frontend:deploy echo-ui-url",
    "sam-build": "cd ../infrastructure && sam build --use-container --template-file ../infrastructure/template.yaml",
    "sam-deploy": "cd ../infrastructure && sam deploy"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^5.2.0",
    "@cloudscape-design/collection-hooks": "^1.0.0",
    "@cloudscape-design/components": "^3.0.0",
    "@cloudscape-design/design-tokens": "^3.0.0",
    "@cloudscape-design/global-styles": "^1.0.0",
    "aws-amplify": "^5.3.10",
    "axios": "^1.3.2",
    "printf": "^0.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.606.0",
    "@aws-sdk/client-s3": "^3.606.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^4.3.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "eslint": "^8.26.0",
    "npm-run-all": "^4.1.5",
    "toml": "^3.0.0",
    "typescript": "^4.6.4",
    "vite": "^5.4.8"
  }
}
